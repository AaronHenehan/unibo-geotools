            <zipfileset excludes="META-INF/*.SF" src="./libs/Apache-HTTP/httpcore-4.2.1.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="./libs/Apache-HTTP/commons-codec-1.6.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="./libs/Apache-HTTP/httpclient-4.2.1.jar"/>
        </jar>
