            <fileset dir="./bin"/>
            <zipfileset excludes="META-INF/*.SF" src="./libs/Apache-HTTP/fluent-hc-4.2.1.jar"/>
            <zipfileset excludes="META-INF/*.SF" src="./libs/Apache-HTTP/httpmime-4.2.1.jar"/>
